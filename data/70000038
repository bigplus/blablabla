<script type="text/javascript">
	var t = null;
	var obj = document.getElementById('reward-box');
    function tan(){
		var obj = document.getElementById('reward-box');
        t = new Dmet.alertBox(obj,{top:'middle',left:'center',animit:false,lock:true})
    }
    var payTid = '70000038';
    var payPrice = '';
    var pay_ewm_img = 'http://m.120ask.com/connect/oauth2/qrcode?url=';
    var pay_sn = '';
    var pay_pid = '';
    var paydocid = '';
    var sweepObj = '';
    var sweepNum = 1;
    $(".reward_btn").click(function(){
    	_czc.push(['_trackEvent', 'post', 'pc_dicengye', 'payans']);
    	payPrice = '1';
		var paydocname = $(this).attr('docname');
		$('.paydocname').html(paydocname);
		pay_sn = $(this).attr('sn');
		pay_pid = $(this).attr('pid');
		paydocid = $(this).attr('doc_id');
		$('.change_price').html(payPrice+'元');
		var docewmurl = encodeURIComponent('http://m.120ask.com/pay/reward/doctor?post_id='+payTid+'&doc_id='+paydocid+'&pid='+pay_pid+'&price='+payPrice+'&sn='+pay_sn);
		var pay_ewm=pay_ewm_img+docewmurl;
		$('#pay_ewm_img').attr('src',pay_ewm);
		pay_ewm = '';
		sweepObj = setInterval("sweepEwm()",2000);
        tan();
        return false;
    })
    $(".close").click(function(){
    	$('#firstprice').addClass("on").siblings().removeClass("on");
    	window.clearInterval(sweepObj);
    	sweepNum = 1;
    	$('.pay-part').show();
        $('.success-part').hide();
        t.closeMask(obj);
    });

    $(function () {
    	$(".pay-part ul li").each(function () {
    		$(this).click(function () {
    			if ($(this).hasClass("txt")) {
    				window.clearInterval(sweepObj);
    		    	sweepNum = 1;
    	            $.ajax({
    	                url: "//m.120ask.com/connect/oauth2/docqr",
    	                async:false,
    	                jsonp: "callback",
    	                dataType: "jsonp",
    	                data: { "docid":paydocid,"w":"146","h":"146"},
    	                success: function( data ) {
    	                    var data = data.html;
    	                    $(".pay-part .right em").css("top","180px");
    	    	            $(".tab-cont .part var").html("微信扫描关注医生");
    	                    $(".tab-cont .part img").attr("src",data);
    	                }
    	            });
    	          }else{
                    payPrice = $(this).attr('price');
                    $('.tab-cont .part var').html('使用微信扫码支付<b class="change_price">'+payPrice+'元</b>');
                    var docewmurl = encodeURIComponent('http://m.120ask.com/pay/reward/doctor?post_id='+payTid+'&doc_id='+paydocid+'&pid='+pay_pid+'&price='+payPrice+'&sn='+pay_sn);
                    var pay_ewm=pay_ewm_img+docewmurl;
                    $('#pay_ewm_img').attr('src',pay_ewm);
                    pay_ewm = '';
                    sweepNum = 1;
    	          }
    			$(this).addClass("on").siblings().removeClass("on");
    		})
    	})
        $(".pay-part .right .tab span").each(function (index) {
            $(this).click(function () {
                $(this).addClass("on").siblings().removeClass("on");
                $(".tab-cont>div:eq("+index+")").css("display","block").siblings().css("display","none")
            })
        })
    })
    
    function sweepEwm()
    {
        if(sweepNum<=25)
        {
        	var url = '/ask/issweepsuc/';
        	$.get(url,{'tid':payTid,'pid':pay_pid,'sn':pay_sn},function (data){
                if(data.code == 200)
                {
                    $('.pay-part').hide();
                    $('.success-part').show();
                    $('#firstprice').addClass("on").siblings().removeClass("on");
                	window.clearInterval(sweepObj);
                	sweepNum = 1;
                	return false;
                }
            },'json');
        	sweepNum++;
        }else{
        	window.clearInterval(sweepObj);
        	sweepNum = 1;
        	return false;
        }
    }
