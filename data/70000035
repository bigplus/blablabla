                    var pay_ewm=pay_ewm_img+docewmurl;
                    $('#pay_ewm_img').attr('src',pay_ewm);
                    pay_ewm = '';
                    sweepNum = 1;
    	          }
    			$(this).addClass("on").siblings().removeClass("on");
    		})
    	})
        $(".pay-part .right .tab span").each(function (index) {
            $(this).click(function () {
                $(this).addClass("on").siblings().removeClass("on");
                $(".tab-cont>div:eq("+index+")").css("display","block").siblings().css("display","none")
            })
        })
    })
    
    function sweepEwm()
    {
        if(sweepNum<=25)
        {
        	var url = '/ask/issweepsuc/';
        	$.get(url,{'tid':payTid,'pid':pay_pid,'sn':pay_sn},function (data){
                if(data.code == 200)
                {
                    $('.pay-part').hide();
                    $('.success-part').show();
                    $('#firstprice').addClass("on").siblings().removeClass("on");
                	window.clearInterval(sweepObj);
                	sweepNum = 1;
                	return false;
                }
            },'json');
        	sweepNum++;
        }else{
        	window.clearInterval(sweepObj);
        	sweepNum = 1;
        	return false;
        }
    }
