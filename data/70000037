    var payTid = '70000037';
    var payPrice = '';
    var pay_ewm_img = 'http://m.120ask.com/connect/oauth2/qrcode?url=';
    var pay_sn = '';
    var pay_pid = '';
    var paydocid = '';
    var sweepObj = '';
    var sweepNum = 1;
